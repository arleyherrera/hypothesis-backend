{
  "name": "hypothesis-backend",
  "version": "1.0.0",
  "description": "Backend para aplicación de gestión de hipótesis",
  "main": "server.js",
  "scripts": {
    "start": "npm run migrate && node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "build": "echo 'No build step required'",
    "postinstall": "echo 'Dependencies installed successfully'",
    "docs:generate": "node scripts/generateDocs.js",
    "test:detailed": "node tests/execution/runners/DetailedConsoleRunner.js",
    "test:report": "node tests/execution/reports/ReportGenerator.js",
    "test:auth": "node tests/execution/runTests.js --tags auth",
    "test:hypothesis": "node tests/execution/runners/ScenarioRunner.js hypothesis-lifecycle",
    "test:complete": "node tests/execution/runTests.js",
    "test:scenario": "node tests/execution/runners/ScenarioRunner.js",
    "test:function": "node tests/execution/runTests.js --function",
    "test:tags": "node tests/execution/runTests.js --tags",
    "test:priority": "node tests/execution/runTests.js --priority high",
    "test:security": "node tests/execution/runTests.js --tags security",
    "test:performance": "node tests/execution/runTests.js --tags performance",
    "test:all": "node tests/execution/runners/MainRunner.js",
    "test:user-journey": "node tests/execution/runners/ScenarioRunner.js user-journey",
    "test:ai": "node tests/execution/runners/ScenarioRunner.js ai-generation",
    "test:detailed:save": "node tests/execution/runners/DetailedConsoleRunner.js > test-output.txt 2>&1",
    "test:artifacts": "node tests/execution/runners/ArtifactTestRunner.js",
    "test:artifacts:detailed": "node tests/execution/runners/ArtifactTestRunner.js > test-artifacts-detailed.txt 2>&1",
    "validate:coherence": "node tests/validation/coherenceValidation.js",
    "validate:auth": "node tests/validation/authenticationValidation.js",
    "validate:generation": "node tests/validation/artifactGenerationValidation.js",
    "validate:all": "node tests/validation/runAllValidations.js",
    "validate:report": "npm run validate:all && open tests/validation/validation-report-*.html",
    "analyze:hypothesis": "node tests/validation/analyzeSpecificHypothesis.js",
    "test:metrics": "node scripts/run-metrics-validation.js",
    "test:metrics:detailed": "jest tests/metrics --verbose",
    "test:objetivo1": "jest tests/metrics/objetivo1-semantic-evaluation.test.js --verbose",
    "test:objetivo3": "jest tests/metrics/objetivo3-ai-generation.test.js --verbose",
    "test:objetivo4": "jest tests/metrics/objetivo4-architecture.test.js --verbose"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/openai": "^0.5.10",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.6",
    "bootstrap-react": "^0.5.0-alpha.14",
    "chromadb": "^2.4.6",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.26",
    "mongoose": "^6.13.8",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.10",
    "openai": "^4.97.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "recharts": "^2.15.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "docx": "^9.5.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
