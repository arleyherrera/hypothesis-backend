{
  "name": "hypothesis-backend",
  "version": "1.0.0",
  "description": "Backend para aplicación de gestión de hipótesis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "build": "echo 'No build step required'",
    "postinstall": "echo 'Dependencies installed successfully'",
    "docs:generate": "node scripts/generateDocs.js",
    "test:detailed": "jest --verbose --runInBand tests/controllers/*.detailed.test.js",
    "test:report": "node tests/runTestsWithReport.js",
    "test:auth": "jest tests/controllers/authController.detailed.test.js --verbose",
    "test:hypothesis": "jest tests/controllers/hypothesisController.detailed.test.js --verbose"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/openai": "^0.5.10",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.6",
    "bootstrap-react": "^0.5.0-alpha.14",
    "chromadb": "^2.4.6",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.26",
    "mongoose": "^6.13.8",
    "morgan": "^1.10.0",
    "openai": "^4.97.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "recharts": "^2.15.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
